<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hnjz.apps.oa.dataexsysnode.model.DataexSysAppid" table="DATAEX_SYS_APPID">
		<id name="uuid" type="java.lang.String">
			<column name="UUID" length="32" />
			<generator class="assigned"></generator>
		</id>
		<property name="appidName" type="java.lang.String">
			<column name="APPID_NAME" length="100" />
		</property>
		<property name="appidCode" type="java.lang.String">
			<column name="APPID_CODE" length="50" />
		</property>
		<property name="createdTime" type="java.util.Date">
			<column name="CREATED_TIME" length="10" />
		</property>
		<property name="remark" type="java.lang.String">
			<column name="REMARK" length="200" />
		</property>
	</class>
</hibernate-mapping>
