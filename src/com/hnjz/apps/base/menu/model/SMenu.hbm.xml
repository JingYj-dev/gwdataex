<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hnjz.apps.base.menu.model.SMenu" table="S_MENU" >
        <id name="menuId" type="java.lang.String">
            <column name="MENU_ID" length="32" />
            <generator class="assigned" />
        </id>
        <property name="funcId" type="java.lang.String">
            <column name="FUNC_ID" length="32" />
        </property>
        <property name="menuName" type="java.lang.String">
            <column name="NAME" length="50" />
        </property>
        <property name="parentId" type="java.lang.String">
            <column name="PARENT_ID" length="32" />
        </property>
        <property name="url" type="java.lang.String">
            <column name="URL" length="255"/>
        </property>
        <property name="openFlag" type="java.lang.String">
            <column name="OPEN_FLAG" length="10" />
        </property>
        <property name="sysId" type="java.lang.String">
            <column name="SYS_ID" length="20" />
        </property>
        <property name="orderNum" type="java.lang.Integer">
            <column name="ORDER_NUM" />
        </property>
        <property name="icon" type="java.lang.String">
            <column name="ICON" length="50" />
        </property>
    </class>
</hibernate-mapping>
